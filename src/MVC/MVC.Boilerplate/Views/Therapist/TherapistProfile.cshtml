        </div>

        <div class="circle circle-lg w-h-25" style="position:absolute;top:40%;">
        <img src="https://img.freepik.com/premium-vector/businessman-profile-cartoon_18591-58481.jpg" class="w-h-25" height="200" alt="THProfileImage">
        </div>*@

        @*<div>
        <div class="mh-25 d-inline-block">
        <img src="https://as1.ftcdn.net/v2/jpg/05/36/22/96/1000_F_536229631_kmKD3E6ffddMdXRgHGrae66JYpRM1YtD.jpg" class="card-img-top" height="200" alt="TherapistCoverPage">
        </div>

        <div class="circle circle-lg w-h-25" style="position:absolute;top:40%;">
        <img src="https://img.freepik.com/premium-vector/businessman-profile-cartoon_18591-58481.jpg" class="w-h-25" height="200" alt="THProfileImage">
        </div>
        </div>*@


        <div class="mh-25 d-inline-block" style="position:relative">

            <div class="circle circle-lg w-h-25" style="position:absolute;top:70%">
                <img src="https://img.freepik.com/premium-vector/businessman-profile-cartoon_18591-58481.jpg" class="w-h-25" height="200" alt="THProfileImage">
            </div>

            <img src="https://as1.ftcdn.net/v2/jpg/05/36/22/96/1000_F_536229631_kmKD3E6ffddMdXRgHGrae66JYpRM1YtD.jpg" class="card-img-top" height="400" alt="TherapistCoverPage"> 
            @*<img src="<img src="https://as1.ftcdn.net/v2/jpg/05/36/22/96/1000_F_536229631_kmKD3E6ffddMdXRgHGrae66JYpRM1YtD.jpg" class="card-img-top" height="400" alt="TherapistCoverPage"> " class="card-img-top" height="400" alt="TherapistCoverPage">*@
        </div>

        
        <div class="d-flex justify-content-end" id="imgicon">
            <img src="data:image/jpeg;base64," class="w-h-25" width="30" height="30" alt="THProfileImage">
            <img src="data:image/png;base64," class="w-h-25" width="50" height="30" alt="THProfileImage">
            <img src="data:image/png;base64," class="w-h-25" width="30" height="30" alt="THProfileImage">

        </div>

        <div class="card-body mt-5">
            @*<h5 class="card-title">Therapist Name</h5>
            <label>Category : </label><p class="card-text"></p>
            <label>URL - </label><p class="card-text"></p>
            <a href="#">Request a private Session</a>

            <p></p>

            <div class="d-grid gap-2">
                <button class="btn btn-primary rounded-pill" type="button">Subscribe</button>
            </div>*@

            @Html.Partial("Partial/_TherapistPartial.cshtml",Model.Therapist)

            <hr>

            <div class="mt-1">
                <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
                    <li class="nav-item col-3" role="presentation">
                        <button class="nav-link active my-2" id="pills-videos-tab" data-bs-toggle="pill" data-bs-target="#pills-videos" type="button" role="tab" aria-controls="pills-videos" aria-selected="true">Videos</button>
                    </li>
                    <li class="nav-item col-3" role="presentation">
                        <button class="nav-link my-2" id="pills-documents-tab" data-bs-toggle="pill" data-bs-target="#pills-documents" type="button" role="tab" aria-controls="pills-documents" aria-selected="false">Documents</button>
                    </li>
                    <li class="nav-item col-3" role="presentation">
                        <button class="nav-link my-2" id="pills-images-tab" data-bs-toggle="pill" data-bs-target="#pills-images" type="button" role="tab" aria-controls="pills-images" aria-selected="false">Images</button>
                    </li>
                    <li class="nav-item col-3" role="presentation">
                        <button class="nav-link my-2" id="pills-products-tab" data-bs-toggle="pill" data-bs-target="#pills-products" type="button" role="tab" aria-controls="pills-products" aria-selected="false">Products</button>
                    </li>
                </ul>
                <div class="tab-content" id="pills-tabContent">
                    <div class="tab-pane fade show active" id="pills-videos" role="tabpanel" aria-labelledby="pills-videos-tab">
                        @Html.Partial("Partial/_VideoPartial.cshtml",Model.VideoList)
                    </div>
                    <div class="tab-pane fade" id="pills-documents" role="tabpanel" aria-labelledby="pills-documents-tab">
                        @Html.Partial("Partial/_DocumentPartial.cshtml",Model.DocumentList)
                    </div>
                    <div class="tab-pane fade" id="pills-images" role="tabpanel" aria-labelledby="pills-images-tab">
                        @Html.Partial("Partial/_ImagePartial.cshtml",Model.ImageList)
                    </div>
                    <div class="tab-pane fade" id="pills-products" role="tabpanel" aria-labelledby="pills-documents-tab">
                        @Html.Partial("Partial/_ProductPartial.cshtml",Model.ProductList)
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>









